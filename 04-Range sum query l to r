#include<stdio.h>
#include<iostream>
using namespace std;

#define max 100000
int array[max]={0};
int prefix[max]={0};

int main(){
int n;
cin>>n;
for(int i=0;i<n;i++){
    cin>>array[i];
}

prefix[0]=array[0];
for(int i=1;i<n;i++){
    prefix[i]=prefix[i-1]+array[i];
}

int l,r;
cin>>l>>r;
int result;
result=prefix[r]-prefix[l-1];
cout<<result;


    return 0;
}