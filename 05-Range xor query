#include<stdio.h>
#include<iostream>
using namespace std;
#define max 100000
int array[max]={0};
int prefix_xor[max]={0};
int main(){
    int n;
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>array[i];
    }

    prefix_xor[0]=array[0];
    for(int i=1;i<n;i++){
        prefix_xor[i]=prefix_xor[i-1]^array[i];
    }

    int l,r,result;
    cin>>l>>r;

    result=prefix_xor[r]^prefix_xor[l-1];
    cout<<result;






    return 0;
}